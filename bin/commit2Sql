#!/usr/bin/python3
# -*-coding:utf-8 -*-
import sys
import configparser


def __error_and_exit(error_message: str, suggestion: str = None):
    print(f'ERROR: {error_message}')
    print(f'\tSUGGESTION:{suggestion}')
    exit(-1)

config = configparser.ConfigParser()

parameters = sys.argv

if len(parameters) == 1:
    __error_and_exit('')

